    "operands": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_operand"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": " "
                  },
                  {
                    "type": "CHOICE",

    "attribute": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^\\s,)(]+"
        }
      }
    },
    "macro": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]+\\([^#]*\\)"

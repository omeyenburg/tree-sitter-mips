    "directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "mnemonic",
          "content": {
            "type": "SYMBOL",
            "name": "mnemonic"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[ \\t]+"

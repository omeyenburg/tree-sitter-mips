                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "attributes",
                      "content": {
                        "type": "SYMBOL",
                        "name": "attributes"
                      }
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "mnemonic": {
      "type": "PATTERN",
      "value": "[.][a-z_]+"
    },
    "_attrsep": {
      "type": "CHOICE",
      "members": [

        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": " "
                },
                {
                  "type": "STRING",
                  "value": "\t"
                }
              ]
